{
  "name": "@sheriff-sale/database",
  "version": "1.0.0",
  "license": "MIT",
  "main": "./dist/index",
  "module": "./dist/index.mjs",
  "types": "./dist/index",
  "files": [
    "dist/**"
  ],
  "scripts": {
    "build": "tsup",
    "clean": "rimraf .turbo node_modules dist",
    "db:generate": "drizzle-kit generate:pg",
    "db:migrate": "tsx src/migrate.ts",
    "db:pull": "drizzle-kit pull:pg",
    "db:push": "drizzle-kit push:pg",
    "db:studio": "drizzle-kit studio",
    "dev": "tsup --watch",
    "lint": "eslint \"src/**/*.ts\"",
    "prebuild": "npm run db:generate",
    "predev": "npm run db:generate"
  },
  "dependencies": {
    "drizzle-orm": "0.31.2",
    "postgres": "3.4.4"
  },
  "devDependencies": {
    "@sheriff-sale/config": "file:./packages/config",
    "@sheriff-sale/tsconfig": "file:./packages/tsconfig",
    "dotenv": "16.4.5",
    "drizzle-kit": "0.22.7",
    "pg": "8.12.0",
    "rimraf": "5.0.7",
    "tsup": "8.1.0"
  }
}
