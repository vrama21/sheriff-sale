{
  "name": "@sheriff-sale/database",
  "version": "1.0.0",
  "license": "MIT",
  "main": "./dist/index",
  "module": "./dist/index.mjs",
  "types": "./dist/index",
  "files": [
    "dist/**"
  ],
  "scripts": {
    "build": "tsup",
    "clean": "rimraf .turbo node_modules dist",
    "db:generate": "drizzle-kit generate:pg",
    "db:pull": "drizzle-kit pull:pg",
    "db:push": "drizzle-kit push:pg",
    "dev": "tsup --watch",
    "lint": "eslint \"src/**/*.ts\"",
    "prebuild": "npm run db:generate",
    "predev": "npm run db:generate"
  },
  "dependencies": {
    "drizzle-orm": "^0.29.3",
    "postgres": "^3.4.3"
  },
  "devDependencies": {
    "@sheriff-sale/config": "*",
    "@sheriff-sale/tsconfig": "*",
    "dotenv": "^16.4.1",
    "drizzle-kit": "^0.20.14",
    "eslint": "^8.12.0",
    "rimraf": "^3.0.2",
    "tsup": "8.0.1",
    "tsx": "4.7.0",
    "typescript": "5.3.3"
  }
}
