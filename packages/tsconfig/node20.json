{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "Node 20",
  "extends": "./base.json",
  "compilerOptions": {
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "lib": ["ES2022"],
    "module": "ES6",
    "skipLibCheck": true,
    "strict": true,
    "target": "ES2022"
  }
}