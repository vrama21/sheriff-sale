{
  "private": true,
  "name": "sheriff-sale",
  "workspaces": [
    "backend",
    "frontend",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "deploy": "turbo run deploy",
    "db:generate": "turbo run db:generate",
    "db:migrate": "turbo run db:migrate",
    "db:pull": "turbo run db:pull",
    "db:push": "turbo run db:push",
    "db:studio": "turbo run db:studio",
    "dev": "turbo run dev",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "lint": "turbo run lint",
    "test": "turbo run test"
  },
  "devDependencies": {
    "@sheriff-sale/config": "file:./packages/config",
    "@types/node": "^20.4.5",
    "esbuild": "^0.16.5",
    "prettier": "^2.5.1",
    "tsx": "^3.7.1",
    "turbo": "^2.0.5"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "packageManager": "npm@10.5.2"
}
