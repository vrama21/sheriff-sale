{
  "private": true,
  "name": "sheriff-sale",
  "workspaces": [
    "backend",
    "frontend",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "deploy": "turbo run deploy",
    "db:generate": "turbo run db:generate",
    "db:migrate": "turbo run db:migrate",
    "db:pull": "turbo run db:pull",
    "db:push": "turbo run db:push",
    "db:studio": "turbo run db:studio",
    "dev": "turbo run dev",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "lint": "turbo run lint",
    "test": "turbo run test"
  },
  "devDependencies": {
    "@types/node": "20.14.9",
    "dotenv": "16.4.5",
    "esbuild": "0.21.5",
    "prettier": "3.3.2",
    "tsx": "4.15.7",
    "turbo": "2.0.5",
    "typescript": "5.5.2"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "packageManager": "npm@10.5.2"
}
